abstract class VehicleAbstract {

	//List of protected attributes.
	protected  String color;
	protected  int	year;
	protected int numberGears;
	
	//Parametric constructor.
	VehicleAbstract(String color, int year, int	numberGears) {
		this.color = color;
		this.year = year;
		this.numberGears = numberGears;
	}
	
	//Creating abstarct accessors for Color, Year, and Gears.
	abstract String getColor();
	abstract int getYear();
	abstract int getNumberGears();

	//Creating abstarct mutators for Color, Year, and Gears.
	abstract void setColor(String color);
	abstract void setYear(int year);
	abstract void setNumberGears(int numberGears);

}
